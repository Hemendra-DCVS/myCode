import java.util.* ;
import java.io.*; 
public class Solution{
    public static int fibonacciNumber(int n){
        // Write your code here.
         if(n == 1 || n==2){
            return 1;
        }
        int fib1=1;
        int fib2=1;
       
        int fib = 1;
        for(int i=3; i<=n;i++){
            
            fib = (fib1 + fib2) % 1000000007;
            fib2 = fib1;
            fib1 = fib;
           
          
        }
      
        return fib;
    }
}