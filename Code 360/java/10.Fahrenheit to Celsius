import java.util.* ;
import java.io.*; 
class Solution {
	
	public static void main(String args[]) {
		
		// Write code here
		Scanner s = new Scanner(System.in);
		int st = s.nextInt();
		int e = s.nextInt();
		int step = s.nextInt();

		for(int i =st; i<=e; i=i+step){
			int c = (i - 32) * 5/9;
			if(c<=0){
				System.out.println(i + " " + (int)Math.floor(c));
			}else{
				System.out.println(i + " " + (int)Math.ceil(c));
			}
		}

	}
}