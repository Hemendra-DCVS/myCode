import java.util.* ;
import java.io.*; 
class Solution {
	
	public static void main(String args[]) {
		
		// Write code here
		Scanner s = new Scanner(System.in);
		int a = s.nextInt();
		int b = s.nextInt();
		int c = s.nextInt();
		int largest = 0;
		if(a > b && a > c){
			largest = a;
		}else if (b > a && b > c){
			largest = b;
		}else{
			largest = c;
		}
		System.out.println(largest);
	}
}