public class Solution {	

	public static int pairSum(int[] arr, int num) {
		//Your code goes here
        int pair = 0;
        for(int i=0; i<arr.length;i++){
            int ele = arr[i];
            for(int j=i+1; j<arr.length;j++){
                if(ele + arr[j] == num){
                    pair++;
					// System.out.println(i + " " + "," + j);
                }
            }
        }
        return pair;
	}
}
//tc: O(n^2)