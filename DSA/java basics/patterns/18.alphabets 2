import java.util.*;
public class Solution {
	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		int n = sc.nextInt();
		char p = 65;
		for(int i = 0; i<n; i++){
			for(int j = 0; j<=i; j++){
				System.out.print(p);
				p++;
			}
			System.out.println();
			p = (char) (p - i);
        }
	}
}
