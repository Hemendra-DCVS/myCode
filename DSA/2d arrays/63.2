
public class Solution {

	public static void totalSum(int[][] mat) {
		//Your code goes here
		int length = mat.length;
		int total = 0;
		for(int i=0;i<mat.length;i++){
			for(int j=0; j<mat.length;j++){
				total = total + mat[i][j];
			}
		}
		System.out.print(total);
		System.out.println();
	}

}