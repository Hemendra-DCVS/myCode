import java.util.*;
public class Solution {
	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		int n = sc.nextInt();
		  char p = (char) ('A' + n - 1);
		for(int i = 1; i<=n; i++){
			for(int j = 1; j <= i; j++){
				System.out.print(p);
				p++;
			}
			System.out.println();
			p = (char) ('A' + n - i - 1);
		}
	}
}
