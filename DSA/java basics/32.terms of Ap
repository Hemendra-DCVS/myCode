import java.util.*;
public class Main {
	
	public static void main(String[] args) {
		
        Scanner sc = new Scanner(System.in)	;
        int n = sc.nextInt();
        int value = 1;
		int maxCount = 0;
        for(int i = 1; maxCount<n; i++){
            value = 3 * i + 2;
            if(value%4 !=0){
                System.out.print(value + " ");
				maxCount++;
            }
        }

	}
}
