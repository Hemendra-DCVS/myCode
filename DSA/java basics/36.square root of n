import java.util.*;
public class Main {
	public static void main(String[] args) {
        Scanner sc = new Scanner(System.in)	;
        int n = sc.nextInt();
		for(int i = 0; i<=n; i++){
			int sum = i * i;
			if(sum == n){
				System.out.print(i);
				break;
			}
			if(sum >= n){
				System.out.print(i-1);
				break;
			}
		}
	}
}