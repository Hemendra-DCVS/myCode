public class Solution {

	public static int findDuplicate(int[] arr) {
		//Your code goes here
        int[] arr2 = new int[arr.length];
        for(int i=0; i<arr.length;i++){
            int value = arr[i];
            arr2[value]++;
        }
        for(int i=0; i<arr2.length; i++){
            if(arr2[i] > 1){
                return i;
            }
        }
        return -1;
	}
}