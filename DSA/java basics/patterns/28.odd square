
import java.util.*;
public class Solution {
	public static void main(String[] args) {
		
		Scanner sc = new Scanner(System.in);
		int n = sc.nextInt();
        int maxvalue = (n*2) -1;
        for(int i=1; i<=n; i++){
             int value = (i * 2) -1;
            for(int j = 1; j<=n; j++){
                //what to print   
                System.out.print(value);
                value = value+2;
                if(value > maxvalue){
                    value = 1;
                }
            }
            System.out.println();
        }
	}
}
