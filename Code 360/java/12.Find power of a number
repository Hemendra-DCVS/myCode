import java.util.* ;
import java.io.*; 
class Solution {
	
	public static void main(String args[]) {
		
		// Write code here
		Scanner s = new Scanner(System.in);
		int x = s.nextInt();
		int n = s.nextInt();

		int sum=1;

		for(int i=0; i<n; i++){
			sum = sum*x;
			
			
		}

		System.out.println(sum);
	}
}