
public class Solution {
    public static void swapAlternate(int arr[]) {
        int even = 0;
        int odd = 1;
        int temp = 0;
        while(even < arr.length && odd < arr.length){
                temp = arr[even];
                arr[even] = arr[odd];
                arr[odd] = temp;
                even += 2;
                odd += 2;
        }
    }
}