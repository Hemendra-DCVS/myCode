//print sequence of numbers 3n+2
import java.util.*;

public class Main {
	
	public static void main(String[] args) {
		// Write your code here
        Scanner sc = new Scanner(System.in)	;
	    int n = sc.nextInt();
        int ans = 0;
		int count = 0;
		  for(int i = 1; count<n; i++ ){
            ans = 3 * i + 2;
            if ( ans%4 != 0){
                System.out.print(ans + " ");
				count++;
            }
        }
	}
}
